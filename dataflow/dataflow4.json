	"name": "dataflow4",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDataset_6yi",
						"type": "DatasetReference"
					},
					"name": "source1",
					"description": "student details austin"
				},
				{
					"dataset": {
						"referenceName": "SourceDataset_6yi",
						"type": "DatasetReference"
					},
					"name": "source2",
					"description": "student details ok"
				}
			],
			"sinks": [
				{
